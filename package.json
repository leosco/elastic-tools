    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "husky": "^0.14.3",
    "nodemon": "^1.17.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.46",
    "chance": "^1.0.16",
    "elasticsearch": "^14.2.2",
